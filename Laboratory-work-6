#5-вариант
#1.
a=[3,6,5,2,10,34,12,5,26,7]
m = None
for x in a:
    if x % 2 != 0:
        if m is None or x>m:
            m=x
print('Максимум нечетных=', m)

#2.
A = [
    [5, 2, 7, 1],
    [4, 9, 6, 3],
    [8, 5, 2, 4],
    [1, 3, 7, 6]
]
n = 4
m = A[0][n-1]
for i in range(n):
    if A[i][n-1-i] > m:
        m = A[i][n-1-i]
print("Максимум по дополнительной диагонали =", m)
